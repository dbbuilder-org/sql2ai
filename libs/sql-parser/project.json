{
  "name": "sql-parser",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/sql-parser/src",
  "projectType": "library",
  "tags": ["scope:lib", "type:sql"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/sql-parser",
        "main": "libs/sql-parser/src/index.ts",
        "tsConfig": "libs/sql-parser/tsconfig.lib.json",
        "assets": ["libs/sql-parser/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/sql-parser/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/sql-parser/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
