{
  "name": "migration-engine",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/migration-engine/src",
  "projectType": "library",
  "tags": ["scope:lib", "type:sql"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/migration-engine",
        "main": "libs/migration-engine/src/index.ts",
        "tsConfig": "libs/migration-engine/tsconfig.lib.json"
      }
    },
    "lint": { "executor": "@nx/eslint:lint", "options": { "lintFilePatterns": ["libs/migration-engine/**/*.ts"] } },
    "test": { "executor": "@nx/jest:jest", "options": { "jestConfig": "libs/migration-engine/jest.config.ts", "passWithNoTests": true } }
  }
}
